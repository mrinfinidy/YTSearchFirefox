{
    "manifest_version": 2,
    "name": "YouTube Quick Search",
    "version": "1.0",
    "permissions": [
        "contextMenus"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.mozilla.org/*"],
            "js": ["background.js"]
        }
    ], 
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    }
}
