{
    "manifest_version": 2,
    "name": "YouTube Quick Search",
    "version": "1.2",
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    }
}
